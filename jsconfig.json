{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~containers/*": [
                "src/containers/*"
            ],
            "~contexts/*": [
                "src/contexts/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~Product/*": [
                "src/components/Product/*"
            ]
        }
    }
}